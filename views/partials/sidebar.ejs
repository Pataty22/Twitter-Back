<aside class="position-fixed">
  <div>
    <div class="my-3 mb-md-0 me-md-auto link-dark text-decoration-none mainIcon">
      <a href="/">
        <div class="mb-4 p-1">
          <div><%- include ("../../public/img/icons/twitter-logo.svg") %></div>
        </div>
      </a>

      <div class="mb-4 p-1">
        <a href="/" class="nav-link" aria-current="page">
          <span><%- include ("../../public/img/icons/home.svg") %></span>
          <span class="home" style="padding-left: 10px">Home</span>
        </a>
      </div>
      <div class="mb-4" style="padding-left: 8px">
        <a href="/usuarios/<%= loggedUser.id %>" class="nav-link link-dark">
          <span><%- include ("../../public/img/icons/profile.svg") %></span>
          <span class="profile" style="padding-left: 12px">Profile</span>
        </a>
      </div>
    </div>
  </div>
  <div class="d-flex flex-column">
    <div>
      <button
        type="submit"
        class="btn btn-login rounded-pill tweetButton"
        style="background-color: var(--color-principal); width: 12.3rem"
      >
        <a class="btn-logout" href="/"
          ><span class="tweetText">Tweet</span
          ><span class="tweetImage d-none"
            ><%- include ("../../public/img/icons/write.svg") %></span
          ></a
        >
      </button>
    </div>
    <div>
      <button type="submit" id="logoutButton" class="btn btn-danger rounded-pill mb-3">
        <a class="btn-logout" href="/logout"
          ><span class="logoutText">Logout</span
          ><i
            class="fa-solid fa-arrow-right-from-bracket d-none logoutImage"
            style="color: #ffffff"
          ></i
        ></a>
      </button>
    </div>
  </div>
</aside>
